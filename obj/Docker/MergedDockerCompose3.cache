name: dockercompose3707579542090148246
services:
  db:
    container_name: Zurich.Db
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Express
      SA_PASSWORD: Admin123*
    image: mcr.microsoft.com/mssql/server:2022-latest
    networks:
      zurich-network: null
    ports:
      - mode: ingress
        target: 1433
        published: "1433"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\franc\source\repos\PruebaZurich\.containers\database
        target: /var/opt/mssql/data
        bind:
          create_host_path: true
  frontend:
    build:
      context: C:\Users\franc\source\repos\PruebaZurich\prueba-zurich
      dockerfile: Dockerfile
    container_name: Zurich.Front
    depends_on:
      pruebazurich:
        condition: service_started
        required: true
    networks:
      zurich-network: null
    ports:
      - mode: ingress
        target: 80
        published: "4200"
        protocol: tcp
  pruebazurich:
    build:
      context: C:\Users\franc\source\repos\PruebaZurich
      dockerfile: PruebaZurich/Dockerfile
    container_name: Zurich.Back
    depends_on:
      db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: pruebazurich
    networks:
      zurich-network: null
    ports:
      - mode: ingress
        target: 80
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\franc\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\franc\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\franc\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\franc\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  zurich-network:
    name: dockercompose3707579542090148246_zurich-network
    driver: bridge